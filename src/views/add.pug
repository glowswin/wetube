extends partials/base
block content
    h1 Add Movie  
    form(method="POST", enctype="multipart/form-data")
        input(name="title",type="text",placeholder="타이틀")
        textarea(name="summary",placeholder="내용")
        input(name="movie",type="file")
        input(name="img",type="file")
        input(name="genres",type="text",placeholder="장르")
        input(type="submit",value="추가")